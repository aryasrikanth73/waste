    <!-- Copy Button -->
    <button onclick="copyText()" class="copy-button">Copy</button>

    <script>
        var counter = 2;
        var counter2 = 2;

        function submitText() {

            var inputValue = document.getElementById("inputText1").value;


            var outputElement = document.getElementById("outputText1");


            var li = document.createElement("li");
            var a = document.createElement("a");
            var text = document.createTextNode('<li><a href="#' + counter + '">' + inputValue + '</a></li>');
            a.appendChild(text);
            li.appendChild(a);


            outputElement.appendChild(li);


            counter++;



        }

        function submitText2() {

            var inputValue = document.getElementById("inputText1").value;
            console.log(inputValue)

            var outputElement = document.getElementById("outputText2");


            var li = document.createElement("li");
            var a = document.createElement("a");
            var text = document.createTextNode('<div id="' + counter2 + '"><h2>' + inputValue + '</h2><p class="para_blog"></p></div>');
            a.appendChild(text);
            li.appendChild(a);


            outputElement.appendChild(li);


            counter2++;



        }
        function submitTextable() {

            submitText();
            submitText2();
            document.getElementById("inputText1").value = "";

        }


        function copyText() {
            var outputText1 = document.getElementById("outputContainer").innerText;
            navigator.clipboard.writeText(outputText1)
                .then(() => {
                    alert("Text copied to clipboard!");
                })
                .catch(err => {
                    console.error('Failed to copy text: ', err);
                });
        }
    </script>
